FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Nb_bin_01;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    "(outlet|atmosphere)"
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    bottom
    {
        type            zeroGradient;
    }

    inlet
    {
//        type            zeroGradient;
        type            fixedValue;
        value           uniform 0;
    }

    empty
    {
        type empty;
    }
}



